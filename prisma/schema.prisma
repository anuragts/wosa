// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(uuid())
  email String  @unique
  name  String?
}

model OpenSourceAlter {
  id String @id @default(uuid())
  name String
  url String
  github String
  imgUrl String
  closedSrc ClosedSource[]
}

model ClosedSource {
  id String @id @default(uuid())
  name String
  imgUrl String
  alternatives OpenSourceAlter[]
}
